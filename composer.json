{
  "name": "andrew72ru/yii2-mongodb-rbac",
  "description": "RBAC management with MongoDB module for Yii2",
  "keywords": ["yii2", "yii2-rbac", "rbac management", "mongodb", "rbac gui", "yii rbac", "rbac module"],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "source": "https://github.com/andrew72ru/yii2-mongodb-rbac"
  },
  "authors": [
    {
      "name": "Andrew Zhdanovskih",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": ">=2.0.9",
    "yiisoft/yii2-mongodb": "~2.1.0",
    "kartik-v/yii2-widget-select2": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "andrew72ru\\rbac\\": ""
    }
  },
  "extra": {
    "bootstrap": "andrew72ru\\rbac\\Bootstrap",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "asset-vcs-driver-options": {
      "github-no-api": true
    },
    "asset-pattern-skip-version": "(-build)"
  }
}
